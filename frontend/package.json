{
  "name": "vue-ts-starter",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue --fix src",
    "prettier": "prettier --write --ignore-unknown '**/*'",
    "stylelint": "stylelint --fix 'src/**/*.{scss,less,css}'",
    "commit": "git-cz",
    "release": "npx bumpp --push --tag --commit 'release: v'",
    "postinstall": "npx msw init public --save",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.1.4",
    "@unocss/reset": "^0.50.4",
    "@vueuse/core": "^8.9.4",
    "@vueuse/integrations": "^8.9.4",
    "axios": "^0.27.2",
    "element-plus": "^2.3.0",
    "pinia": "^2.0.33",
    "universal-cookie": "^4.0.4",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "workbox-core": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-window": "^6.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@iconify-json/carbon": "^1.1.16",
    "@intlify/unplugin-vue-i18n": "^0.9.2",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "autoprefixer": "^10.4.14",
    "bumpp": "^7.2.0",
    "commitizen": "^4.3.0",
    "commitlint-config-cz": "^0.13.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.9.2",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^12.5.0",
    "msw": "^0.40.2",
    "postcss": "^8.4.21",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.4",
    "rollup": "^2.79.1",
    "sass": "^1.59.2",
    "standard-version": "^9.5.0",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "unocss": "^0.50.4",
    "unplugin-auto-import": "^0.7.2",
    "unplugin-vue-components": "^0.19.9",
    "unplugin-vue-macros": "^1.10.0",
    "vite": "^3.2.5",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-pwa": "^0.13.3",
    "vue-tsc": "^0.39.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.js"
    }
  },
  "packageManager": "pnpm@7.11.0",
  "msw": {
    "workerDirectory": "public"
  }
}
